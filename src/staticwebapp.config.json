{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/test.html",
        "allowedRoles": ["authenticated"]
      },
      {
        "route": "/private.html",
        "allowedRoles": ["customrole"]
      },
      {
        "route": "/login",
        "redirect": "jwlogin.html"
      },
      {
        "route": "/logout",
        "redirect": "/.auth/logout"
      },
      {
        "route": "/*",
        "allowedRoles": ["anonymous"]
      }
    ],
    "responseOverrides": {
      "400": {
        "rewrite": "/400.html"
      },
      "401": {
        "rewrite": "/login",
        "status": 302
      },
      "403": {
        "rewrite": "/403.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }
